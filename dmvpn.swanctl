connections {
	dmvpn {
		mobike = no
		dpd_delay = 15s
		unique = replace
		reauth_time = 13h
		local {
			certs = dmvpn.pem
		}
		remote {
			cacerts = dmvpn.pem
			revocation = ifuri
		}
		children {
			dmvpn {
				local_ts = dynamic[gre]
				remote_ts = dynamic[gre]
				rekey_time = 100m
				mode = transport
				inactivity = 90m
				reqid = 1
			}
		}
	}
}
